{
  "name": "@starpeace/starpeace-documentation-website",
  "version": "0.1.0",
  "description": "Documentation website for STARPEACE multiplayer economic simulation",
  "author": "starpeace-project",
  "license": "CC-BY-4.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "cross-env NODE_ENV=development nuxt dev --port 11005",
    "generate": "cross-env nuxt generate",
    "generate-api-docs": "openapi-generator-cli generate -i ./node_modules/@starpeace/starpeace-server-multiverse-api/api/openapi.yaml -g html -t ./openapi/html-templates/ -o ./.output/public/api-docs/multiverse && cross-env del-cli ./.output/public/api-docs/multiverse/.openapi-generator-ignore ./.output/public/api-docs/multiverse/.openapi-generator",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "nuxt": "^3.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "^3.0.2",
    "@nuxtjs/google-gtag": "^1.0.4",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@starpeace/starpeace-assets": "git://github.com/starpeace-project/starpeace-assets.git#v0.2.24",
    "@starpeace/starpeace-assets-types": "git://github.com/starpeace-project/starpeace-assets-types.git#v0.2.25",
    "@starpeace/starpeace-server-multiverse-api": "git://github.com/starpeace-project/starpeace-server-multiverse-api.git#v0.1.7",
    "@types/lodash": "^4.14.190",
    "@types/luxon": "^3.1.0",
    "bulma": "^0.9.4",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "favicon-switcher": "^1.2.2",
    "lodash": "^4.17.21",
    "luxon": "^3.1.0",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass": "1.56.1",
    "sass-loader": "^13.2.0",
    "terser": "^5.16.1"
  }
}
