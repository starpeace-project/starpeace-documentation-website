{
  "name": "@starpeace/starpeace-documentation-website",
  "version": "0.1.0",
  "description": "Documentation website for STARPEACE multiplayer economic simulation",
  "author": "starpeace-project",
  "license": "CC-BY-4.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "cross-env NODE_ENV=development nuxt dev --port 11005",
    "generate": "cross-env nuxt generate",
    "generate-api-docs": "openapi-generator-cli generate -i ./node_modules/@starpeace/starpeace-server-multiverse-api/api/openapi.yaml -g html -t ./openapi/html-templates/ -o ./.output/public/api-docs/multiverse && del-cli ./.output/public/api-docs/multiverse/.openapi-generator-ignore ./.output/public/api-docs/multiverse/.openapi-generator",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "nuxt": "^3.1.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@starpeace/starpeace-assets": "github:starpeace-project/starpeace-assets.git#v0.2.24",
    "@starpeace/starpeace-assets-types": "github:starpeace-project/starpeace-assets-types.git#v0.3.1",
    "@starpeace/starpeace-server-multiverse-api": "github:starpeace-project/starpeace-server-multiverse-api.git#v0.1.7",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.2.0",
    "bulma": "^0.9.4",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "favicon-switcher": "^1.2.2",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass": "1.57.1",
    "sass-loader": "^13.2.0",
    "vue-gtag-next": "^1.14.0"
  }
}
